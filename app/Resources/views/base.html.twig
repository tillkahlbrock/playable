<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {% stylesheets
            '../app/Resources/public/vendor/bootstrap/dist/css/bootstrap.min.css' filter='cssrewrite'
            '../app/Resources/public/vendor/bootstrap/dist/css/bootstrap-theme.min.css' filter='cssrewrite'
            %}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts%}
            {% javascripts
            '../app/Resources/public/vendor/jquery/dist/jquery.js'
            %}
            <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
</html>
